    "fact = 1\n",
    "def factorial (n):\n",
    "    global fact\n",
    "    fact =1\n",
    "    for i in range (1,n+1):\n",
    "        fact*=i\n",
    "    return fact \n",
    "num=int(input(\"enter the number\"))\n",
    "if num <0:\n",
    "    factorial (num)\n",
    "    print(f\"factorial of {num} is\",fact)"
   ]
